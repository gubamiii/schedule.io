{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/blob-upload",
      "dest": "app.py",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
